<h4 class="mt-3">Reatiler's List</h4>
<div class="table-responsive mt-5 mb-5">
  <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
      <tr>
        <th>Image</th>
        <th>Name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Phone Number</th>
        <th>Delete</th>
      </tr>
    </thead>
   


    {{!-- <div class="card card-body mb-2"> --}}

      <tbody>
         {{#each users}}
        <tr>
           <td>
         <a href = "{{profileimage}}"><img src="{{profileimage}}" style="height:40px;width:40px" class="image-fluid rounded-circle"></a>
               </td>
          <td> {{name}}</td>
          <td> {{email}}</td>
          <td>{{gender}}</td>
          <td>{{address}}</td>
          <td>{{phonenumber}}</td>
          {{!-- <td> <a class="btn btn-dark btn-block mb-2" href="/pgs/edit/{{id}}">Edit</a></td> --}}
          <td>
            <form method="post" action="/admin/{{id}}?_method=DELETE">
              <input type="hidden" name="_method" value="DELETE">
              <input type="submit" class="btn btn-danger btn-block" value="Delete">
            </form>
          </td>

        </tr>
        
    {{/each}}
      </tbody>

      {{!-- <p>{{image}}</p> --}}

      {{!-- </div> --}}
    
  </table>
</div>